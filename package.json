  "private": true,
  "scripts": {
    "start:": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "start:packager": "watchman watch-del-all && react-native start -- --reset-cache",
